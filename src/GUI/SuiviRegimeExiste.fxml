<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="root" nodeOrientation="LEFT_TO_RIGHT" prefHeight="810.0" prefWidth="1315.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.SuiviRegimeExisteController">
   <children>
      <ImageView fitHeight="549.0" fitWidth="492.0" layoutX="475.0" layoutY="95.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/noGif.gif" />
         </image>
      </ImageView>
      <Label layoutX="251.0" layoutY="587.0" text="Mr/Mme {{user.nom}} {{user.prenom}} ! Vous avez déjà un suivi régime non encore expiré !">
         <font>
            <Font name="Cambria" size="24.0" />
         </font>
      </Label>
      <Label layoutX="281.0" layoutY="627.0" text="Terminer votre suivi régime actuel ou bien le supprimer pour avoir acheté de nouveau régime">
         <font>
            <Font name="Cambria" size="22.0" />
         </font>
      </Label>
      <Label fx:id="btn_accueil" alignment="TOP_CENTER" layoutX="493.0" layoutY="729.0" onMouseClicked="#showAccueil" styleClass="btn_accueil" stylesheets="@frontregime.css" text="Accueil">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Label layoutX="776.0" layoutY="729.0" onMouseClicked="#showSuiviRegimeUser" text="Mon suivi Régime">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
